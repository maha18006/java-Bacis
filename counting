import java.util.*;

public class Main {
    public static void main(String[] args) {
      Scanner sc =new Scanner(System.in);
      int num=sc.nextInt();
      int count=0;
      for(int i=2;i<=num/2;i++){
        if(num%i==0){
          count++;
        }
        
      }
      System.out.println(count+2);
    }
}
output:
5
2
