import java.util.*;
public class Main{
  public static void main(String[]args){
  Scanner sc = new Scanner(System.in);
  String str = "innate talent";
  String str1 = "innate talent";
  System.out.println(str.equals(str1));
  }
}
OUTPUT:
true

