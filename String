#Basic
import java.util.*;
public class Main {
	public static void main(String[]args) {
		Scanner sc=new Scanner(System.in);
		String str="innate talent";
		String str1=sc.next();
		sc.nextline();
		String str2=sc.nextline();
		System.out.println(str1);
		System.out.println(str2);
	
	}
}
OUTPUT:
innate
innate talent


compareTO()=int
compareToIgnoreCase()=int
concat()=string
contains()
length()=int
lastIndexOf()=int
indexOf()=int
isEmpty()=boolean
endsWith()=boolean
equals()=boolean
equalsIgnoreCase()=Boolean
replace()=string
replaceFirst()=string
replaceAll()=string
split()=string[]
toLowerCase()=String
toString()=String
toUpperCase()=String
trim()=String
valueOf()=String

2.compareTO()
PROGRAM:
import java.util.*;
public class Main{
  public static void main(String[]args){
  Scanner sc = new Scanner(System.in);
  String str = "innate talent";
  String str1 = "Innate Talent";
  System.out.println(str.compareTo(str1));
  }
}
OUTPUT:
32

3.compareToIgnoreCase():
PROGRAM:
import java.util.*;
public class Main{
  public static void main(String[]args){
  Scanner sc = new Scanner(System.in);
  String str = "innate talent";
  String str1 = "Innate Talent";
  System.out.println(str.compareToIgnoreCase(str1));
  }
}
OUTPUT:
0

4.concat():
PROGRAM:
import java.util.*;
public class Main{
  public static void main(String[]args){
  Scanner sc = new Scanner(System.in);
  String str = "innate talent";
  String str1 = "Innate Talent";
  System.out.println(str.concat(str1));
  }
}
OUTPUT:
innate talentInnate Talent

5.contains()
PROGRAM:
import java.util.*;
public class Main{
  public static void main(String[]args){
  Scanner sc = new Scanner(System.in);
  String str = "innate talent";
  String str1 = "innate talent";
  System.out.println(str.contains(str1));
  }
}
OUTPUT:
true

6.length():
